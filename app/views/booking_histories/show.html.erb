<p id="notice"><%= notice %></p>
<h1> Your Booking History </h1>
<% @booking_history = BookingHistory.where("booking_histories.username = ?",'z').order(:date) %>
<table border="2">
  <tr>
    <th>Booking Id</th>
    <th>Room Num</th>
    <th>Building</th>
    <th>Size</th>
    <th>Date</th>
    <th>Start Time</th>
    <th>End Time</th>
    </tr>
<% @booking_history.each do |mem| %>
      <tr>
        <td><%= "#{mem.id}" %></td>
        <td><%= "#{mem.room_num}" %></td>
        <td><%= "#{mem.building}" %></td>
        <td><%= "#{mem.size}" %></td>
        <td><%= "#{mem.date}" %></td>
        <td><%= "#{mem.start_t}" %></td>
        <td><%= "#{mem.end_t}" %></td>
        </tr>
      <% end %>
</table>

<br>
